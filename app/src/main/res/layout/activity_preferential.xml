<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.lxd.ymxx.app.PreferentialActivity">

    <include layout="@layout/titleset_bar"></include>

    <com.astuetz.PagerSlidingTabStrip
        xmlns:psts="http://schemas.android.com/apk/res-auto"
        android:id="@+id/tabs_preferential"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="#353535"
        psts:pstsDividerColor="#00000000"
        psts:pstsIndicatorColor="@color/tab_press"
        psts:pstsIndicatorHeight="5dp"
        psts:pstsShouldExpand="true"
        psts:pstsUnderlineHeight="0dp" >
    </com.astuetz.PagerSlidingTabStrip>
    <android.support.v4.view.ViewPager
        android:padding="16dp"
        android:id="@+id/loginPager"
        android:layout_width="match_parent"
        android:background="#E8E8E8"
        android:layout_height="match_parent"/>

</LinearLayout>
